# Database Provider: "local" | "supabase"
DATABASE_PROVIDER=local

# Local PostgreSQL (gunakan jika DATABASE_PROVIDER=local)
DATABASE_URL=postgresql://postgres:password@localhost:5432/famfinance

# Supabase PostgreSQL (gunakan jika DATABASE_PROVIDER=supabase)
SUPABASE_DATABASE_URL=postgresql://postgres:[PASSWORD]@db.[PROJECT-ID].supabase.co:5432/postgres

# Supabase Auth (wajib untuk verifikasi JWT, regardless database provider)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Environment
NODE_ENV=development
